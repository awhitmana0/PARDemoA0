# Auth0 Configuration
# Copy this file to .env and update with your Auth0 application details

# Your Auth0 domain (e.g., your-domain.auth0.com)
VITE_AUTH0_DOMAIN=your-domain.auth0.com

# Your Auth0 application client ID
VITE_AUTH0_CLIENT_ID=your-client-id

# Your Auth0 application client secret (for PAR flow)
VITE_AUTH0_CLIENT_SECRET=your-client-secret

# Your Auth0 API audience (optional)
VITE_AUTH0_AUDIENCE=your-api-audience

# Callback URL (should match your Auth0 application settings)
VITE_CALLBACK_URL=http://localhost:5173/callback